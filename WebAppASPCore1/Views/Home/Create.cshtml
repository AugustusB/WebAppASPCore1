
@model HomeDeveloperVM

@{ 
    ViewBag.Title = "Create Developer";
}
<form method="post">
    <div>
        <label asp-for="Name"></label>
        <input asp-for="Name" />
        <span asp-validation-for="Name"></span>
    </div>
    <div>
        <label asp-for="Age"></label>
        <input asp-for="Age" />
        <span asp-validation-for="Age"></span>
    </div>
    <div>
        <label asp-for="Address"></label>
        <input asp-for="Address" />
        <span asp-validation-for="Address"></span>
    </div>
    <div>
        <label asp-for="TypeOfDev"></label>
        <select asp-for="TypeOfDev" asp-items="Html.GetEnumSelectList<eDeveloperType>()"></select>
        <span asp-validation-for="TypeOfDev"></span>
    </div>
    <input type="submit" name="save" value="Save" />
</form>
